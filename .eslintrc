{
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
    },
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    },
    "plugins": {
        "react"
    },
    "extends": "eslint:recommended",
    "rules": {
        "strict": [2, "function"],
        "no-undef": 2,
        "no-console": 0,
        "react/jsx-uses-vars": 1,
        "arrow-parens": [2, "always"]
    }
}
